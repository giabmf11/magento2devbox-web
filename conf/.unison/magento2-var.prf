# Roots of the synchronization
root = /home/magento2
root = /var/www

# Paths to synchronize 
path = magento2
ignore = Path app
ignore = Path bin
ignore = Path dev
ignore = Path lib
ignore = Path vendor
ignore = Path phpserver
ignore = Path setup

# Params
fastercheckUNSAFE = true
ignorelocks = true
numericids = false
log = true
logfile = /home/magento2/unison.var.log
batch = true
fastcheck = true
confirmbigdel = false
ignore = Path */.git
ignore = Path */.idea
ignore = Path */update/vendor/composer/composer/tests/Composer/Test/Autoload/Fixtures/classmap/LargeClass.php
prefer = /home/magento2